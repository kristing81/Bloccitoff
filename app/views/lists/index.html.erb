<h2>Lists</h2>
<div class="list-group">
  <div class="row">
    <div class="col-md-4">
      <% @lists.each do |list| %>
          <div  class="list-group-item">
              <%= link_to list.name, list %>
          </div>
        <% end %>
      </div>
    </div>
    <br/>
    <%= link_to "New list", new_list_path, class: "btn btn-default btn-sm"%>
</div>

