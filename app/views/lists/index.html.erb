<h3>Lists</h3>
  <div class="row">
    <div class="col-md-4">
      <% @lists.each do |list| %>
          <div class="media">
            <div class="media-body">
              <h4 class="media-heading">
                <%= link_to list.name, list %>
              </h4>
            </div>
          </div>
        <% end %>
      </div>
    <%= link_to "New list", new_list_path, class: "btn btn-default"%>
</div>

